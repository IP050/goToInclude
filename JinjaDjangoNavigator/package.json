{
  "name": "JinjaDjangoNavigator",
  "displayName": "GoToInclude",
  "description": "Focuses on enhancing navigation to included or extended HTML template files in environments like Django or Jinja",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.gotoInclude",
    "onLanguage:html",
    "onLanguage:django-html"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.gotoInclude",
        "title": "Go to Included File"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorTextFocus",
          "command": "extension.gotoInclude",
          "group": "navigation"
        }
      ]
    },
      "keybindings": [
    {
      "command": "extension.gotoInclude",
      "key": "ctrl+alt+i",
      "when": "editorTextFocus",
      "mac": "cmd+alt+i"
    }
  ],
  "commands": [
    {
      "command": "extension.gotoInclude",
      "title": "Go to Included File"
    }
  ]

  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.89.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.3.9"
  }
}
